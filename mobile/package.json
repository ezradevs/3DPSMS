{
  "name": "3dpsms-mobile",
  "version": "1.0.0",
  "private": true,
  "main": "expo-entry.js",
  "scripts": {
    "start": "EXPO_USE_POLLING=1 node ./node_modules/expo/bin/cli start",
    "android": "EXPO_USE_POLLING=1 node ./node_modules/expo/bin/cli start --android",
    "ios": "EXPO_USE_POLLING=1 node ./node_modules/expo/bin/cli start --ios",
    "web": "EXPO_USE_POLLING=1 node ./node_modules/expo/bin/cli start --web",
    "web:build": "node ./node_modules/expo/bin/cli export --platform web --clear",
    "web:preview": "npx serve dist"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "^7.6.1",
    "@react-native-community/slider": "^4.4.4",
    "react-native-gesture-handler": "~2.16.1",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.25",
    "@tanstack/react-query": "^5.50.1",
    "dayjs": "^1.11.11",
    "expo": "~54.0.0",
    "expo-blur": "~13.0.2",
    "expo-linear-gradient": "~13.0.2",
    "@react-native-masked-view/masked-view": "^0.2.9",
    "expo-constants": "~18.0.9",
    "expo-document-picker": "~14.0.7",
    "expo-file-system": "~19.0.16",
    "expo-image-picker": "~17.0.8",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0"
  },
  "expo": {
    "install": {
      "exclude": [
        "expo-document-picker"
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "babel-preset-expo": "~54.0.0"
  }
}
